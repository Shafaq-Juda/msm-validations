<!DOCTYPE html>
<html>
  <head>
    <title>MSM Validations</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
  </head>


  <body>
  <% if notice.present? %>
    <%= notice %>
  <% end %>
  <% if alert.present? %>
    <%= alert %>
  <% end %>
    <nav>
      <a href="/">
        Home
      </a>
    </nav>

    <%= yield %>
  </body>
</html>
